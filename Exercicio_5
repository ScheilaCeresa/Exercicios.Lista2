algoritmo "Exer5_Lista2"
var
    ano: inteiro
inicio
      escreva ("Informe o ano: ")
      leia (ano)
      
      se (ano mod 4 = 0) e (ano mod 100<>0) ou (ano mod 400 = 0) entao
         escreva (ano, " é bissexto!")
      senao
           escreva (ano, " não é bissexto!")
      fimse
fimalgoritmo